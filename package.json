{
  "name": "my-turborepo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "build:turbopack": "turbo build --env-mode=loose",
    "dev": "turbo dev",
    "dev:turbopack": "turbo dev --env-mode=loose",
    "lint": "turbo lint",
    "clean": "turbo clean",
    "type-check": "turbo check-types",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.4.2",
    "turbo": "^2.3.3",
    "typescript": "5.5.4"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
